static void VPreLex_load_buffer_state (void );
static void VPreLex_init_buffer (YY_BUFFER_STATE b,FILE *file  );

#define YY_FLUSH_BUFFER VPreLex_flush_buffer(YY_CURRENT_BUFFER )

